    "test-all": "npm ci && npm run build",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "lint": "concurrently \"npm run lint:js\" \"npm run lint:types\"",
    "lint:types": "tsc --noEmit",
    "lint:js": "eslint . --ext js,jsx,ts,tsx",
    "lint:info": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
